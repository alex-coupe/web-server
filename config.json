{
    "listen":["http://localhost:8080/"],
    "documentRoot":"wwwroot",
    "keepAlive":true,
    "timeout":60,
    "maxConnections":3000,
    "logBatchSize":30,
    "logTimeThreshold":30,
    "logMaxFileSize":500000,
    "rateLimit":50,
    "cleanupInterval":300,
    "cors":{
        "allowOptions":true,
        "allowOrigin":"*",
        "allowMethods":"GET, POST, PUT, DELETE, OPTIONS",
        "allowHeaders":"*",
        "allowAuth":true
    }
}